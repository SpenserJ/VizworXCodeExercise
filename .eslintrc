{
  "parser": "babel-eslint",
  "extends": "airbnb",
  "rules": {
    "no-underscore-dangle": ["error", { "allowAfterThis": true }],
    "import/extensions": [
      "error",
      "always",
      {
        "js": "never",
        "jsx": "never",
        "scss": "never"
      }
    ]
  },
  "settings": {
    "import/resolver": ["webpack", "node"]
  }
}
